{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
  "rules": {
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "@typescript-eslint/no-explicit-any": ["off", {
        "fixToUnknown": true
    }],
    "curly": "error",
    "@typescript-eslint/no-inferrable-types": "off"
  }
}
